<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">


<h1><%= test_content %></h1>

<div class="table-responsive col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
    <table class="table">
      <thead>
        <tr>
          <th>Name </th>
          <th>Address</th>
          <th>Number</th>
          <th>Email</th>
          <th>Work</th>
          <th>Status</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach(function(item) {%>
            <tr>                  
                <td> <%=item.name %> </td>  
                <td> <%=item.address %> </td>
                <td> <%=item.number %> </td>  
                <td> <%=item.email %> </td>
                <td> <%=item.work %> </td>  
                <td> <%=item.status %> </td>                
              </tr>
        <% }); %>
      </tbody>
    </table>
  </div>

